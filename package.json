{
  "name": "@muravjev/games",
  "version": "0.1.0",
  "private": true,
  "description": "Games monorepo",
  "author": "Sergey Muravjev <sergey.muravjev@gmail.com> (http://github.com/muravjev)",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf \"**/node_modules\" && pnpm -r clean",
    "fresh": "pnpm clean && pnpm i",
