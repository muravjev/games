    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/ua-parser-js": "^0.7.36",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "jest": "^29.1.2",
