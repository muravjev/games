{
  "name": "tetris-classic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf .next tsconfig.tsbuildinfo .coverage",
    "dev": "next dev",
    "dev-ssl": "next dev -p 3001 | local-ssl-proxy --key .ssl/localhost.key --cert .ssl/localhost.crt --source 3000 --target 3001",
    "build": "next build",
    "test": "jest",
    "test:coverage": "pnpm run test --coverage",
    "export": "next build && next export -o .export && winrar-zip-build.bat",
    "start": "next start",
    "lint": "next lint",
    "sass": "sass src:css --load-path src/styles --no-source-map -w"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.0",
    "@reduxjs/toolkit": "^1.8.3",
    "clsx": "^1.2.1",
    "fast-deep-equal": "^3.1.3",
    "next": "12.2.2",
    "next-i18next": "^11.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.2",
    "ua-parser-js": "^1.0.2"
  },
  "devDependencies": {
    "@indigotree/dart-sass-json-importer": "^0.2.0",
    "@muravjev/eslint-config-app-next": "workspace:*",
    "@muravjev/ts-config-app-next": "workspace:*",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/ua-parser-js": "^0.7.36",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "sass": "^1.53.0",
    "typescript": "4.7.4"
  }
}
