    "@muravjev/configs-ts": "workspace:*",
    "@muravjev/features-fallback": "workspace:*",
    "@muravjev/utils-deferred": "workspace:*",
    "@muravjev/utils-redux-subslice": "workspace:*",
    "@reduxjs/toolkit": "^1.8.6",
    "clsx": "^1.2.1",
    "fast-deep-equal": "^3.1.3",
    "next": "12.3.1",
